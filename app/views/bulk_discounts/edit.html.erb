<h2><%= "Bulk Discount #{@bulk_discount.id}" %></h2>
<%= form_for [@bulk_discount], url: "/merchants/#{@bulk_discount.merchant.id}/bulk_discounts/#{@bulk_discount.id}", method: :patch do |f| %>
  <div class="field">
    <%= f.label :quantity_threshold %>
    <%= f.text_field :quantity_threshold%>
    <br>
    <br>
    <%= f.label :percentage_discount %>
    <%= f.text_field :percentage_discount %>
  </div>
  <%= f.submit "Submit" %>
<% end %>